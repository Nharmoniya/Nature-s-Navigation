t ||(或者) 等于存入的数组\r\nconst hashMap = xObject || [\r\n  {\r\n    logo: \"https://developer.mozilla.org/favicon-192x192.png\",\r\n    url: \"https://developer.mozilla.org/zh-CN/\",\r\n    link: \"MDN\"\r\n  },\r\n  { logo: \"src/Caniuse.jpg\", \r\n    url: \"https://caniuse.com/\",\r\n    link: \"Caniuse\"\r\n  },\r\n];\r\n\r\nconst render= ()=>{\r\n  $siteList.find('li:not(.last)').remove()//把之前的li都找到除了最后一个，然后删除\r\n  hashMap.forEach(node => {\r\n    const $li = $(`<li>\r\n    <a href=\"${node.url}\">\r\n      <div class=\"site\">\r\n        <div class=\"logo\"><img src=${node.logo}/></div>\r\n        <div class=\"link\">${node.link}</div>\r\n      </div>\r\n    </a>\r\n  </li>\r\n    `).insertBefore($lastLi);\r\n  });\r\n}\r\n//调用render\r\nrender();\r\n\r\n$(\".addButton\").on(\"click\", () => {\r\n  let url = window.prompt(\"请问你要添加的网址是啥？\");\r\n  let urlName = window.prompt(\"请问你要添加的网址名称\");\r\n  console.log(url);\r\n  if (url.indexOf(\"http\") != 0) {\r\n    url = \"https://\" + url;\r\n  }\r\n  console.log(url);\r\n  hashMap.push({\r\n    logo:url[0],\r\n    url:url,\r\n    link:urlName,\r\n    logoType:'text',\r\n  })\r\n  //调用render\r\n   render();\r\n});\r\n\r\n//监听用户关闭浏览器的API\r\nwindow.onbeforeunload = ()=>{\r\n  const string = JSON.stringify(hashMap)//把对象变成字符串\r\n  localStorage.setItem('x',string)//把变成字符串的hashmap存入localStorage中名为x\r\n}"},"lineCount":null}},"error":null,"hash":"2048882f27e5cb101e82962b3088a99a","cacheData":{"env":{}}}